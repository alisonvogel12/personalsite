@using System.Web.Optimization
<!DOCTYPE html>
<html>
<head>
    @Styles.Render("~/Content/siteCSS")
    @Styles.Render("~/Content/bootstrapCSS")
    @Styles.Render("~/Content/fontawesomeCSS")
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
</head>
<body>
    <header>
        @Html.Partial("_NavBar")
        <img src="@Url.Content("~/Files/Optimized-SiteImage_V2.png")" class="img-responsive hidden-xs">
        <img src="@Url.Content("~/Files/SiteImage_SmallScreen.png")" class="img-responsive visible-xs">
    </header>
    <br />
    <br />
    <div>
        @RenderBody()
    </div>
    <br>
    <br />
    <br />
    <footer class="footer">
        <div id="navs" class="center hidden-xs">
            <ul>
                <li><a href="https://www.facebook.com/alison.ridgway.7" target="_blank"><i class="fa fa-facebook-f"></i></a></li>
                <li><a href="https://www.linkedin.com/in/alison-vogel-ba03b335" target="_blank"><i class="fa fa-linkedin-square"></i> </a></li>
                <li><a href="https://www.instagram.com/alison_vogel/" target="_blank"><i class="fa fa-instagram"></i></a></li>
            </ul>
        </div>
    </footer>
    @Scripts.Render("~/Scripts/siteJS")
    @Html.Partial("_GoogleAnalytics")
</body>
</html>

